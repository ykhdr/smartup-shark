<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>

<VBox blendMode="MULTIPLY" prefHeight="720" prefWidth="1024" styleClass="bodybg" stylesheets="@../background/style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nsu.fit.ykhdr.smartupshark.controller.ScoreboardController">

    <Button alignment="CENTER" contentDisplay="RIGHT" layoutX="904.0" layoutY="14.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onActionBtnBack" prefHeight="75.0" prefWidth="111.0" stylesheets="@../background/style.css" text="Back" VBox.vgrow="NEVER" />


    <TableView fx:id="scoreboard" nodeOrientation="RIGHT_TO_LEFT" prefHeight="465.0" prefWidth="977.0">
        <columns>
            
            <TableColumn fx:id="scoreColumn" minWidth="0.0" prefWidth="469.0" text="Score">
                <cellValueFactory>
                    <PropertyValueFactory property="score" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="dateColumn" prefWidth="319.0" text="Date">
                <cellValueFactory>
                    <PropertyValueFactory property="date" />
                </cellValueFactory>
            </TableColumn>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>

</VBox>
